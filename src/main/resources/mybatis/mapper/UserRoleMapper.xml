<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.venues.lt.demo.mapper.UserRoleMapper" >
  <resultMap id="BaseResultMap" type="com.venues.lt.demo.model.UserRole" >
    <id column="user_id" property="userId" jdbcType="CHAR" />
    <id column="role_id" property="roleId" jdbcType="INTEGER" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="map" >
    delete from user_role
    where user_id = #{userId,jdbcType=CHAR}
      and role_id = #{roleId,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.venues.lt.demo.model.UserRole" >
    insert into user_role (user_id, role_id)
    values (#{userId,jdbcType=CHAR}, #{roleId,jdbcType=INTEGER})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap" >
    select user_id, role_id
    from user_role
  </select>
</mapper>